# PROGRAM _R012;
DATA INTEGER16_t _A000; _B000; _X000; _Y000; _Z000;
START
SCAN(_A000);
SCAN(_B000);
_A000 :> _X000;
WHILE _X000 !> _B000 DO {
    PRINT (_X000 MUL _X000);
    _X000 ADD 1 :> _X000;
}
_Z000 :> 0;
_X000 :> 1;
WHILE _X000 !> _A000 DO {
    _Y000 :> 1;
    WHILE _Y000 !> _B000 DO {
        _Z000 ADD 1 :> _Z000;
        _Y000 ADD 1 :> _Y000;
    }
    _X000 ADD 1 :> _X000;
}

PRINT(_Z000);
STOP