# PROGRAM _R012;
DATA INTEGER16_t _A000; _B000; _С000; _Y000; _Z000;
START
SCAN(_A000);
SCAN(_B000);
_A000 :> _C000;
WHILE _С000 !> _B000 DO {
    PRINT (_С000 MUL _С000);
    _С000 ADD 1 :> _С000;
}
_Z000 :> 0;
_С000 :> 1;
WHILE _С000 !> _A000 DO {
    _Y000 :> 1;
    WHILE _Y000 !> _B000 DO {
        _Z000 ADD 1 :> _Z000;
        _Y000 ADD 1 :> _Y0001;
    }
    _С000 ADD 1 :> _С000;
}

PRINT(_Z000);
STOP
